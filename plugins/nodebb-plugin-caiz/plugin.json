{
  "id": "nodebb-plugin-caiz",
  "name": "NodeBB Plugin for Caiz",
  "description": "NodeBB Plugin for Caiz",
  "version": "1.0.0",
  "library": "./library.js",
  "languages": "languages",
  "scripts": [
    "static/modal.js",
    "static/follow.js",
    "static/consent-interceptor.js",
    "static/consent.js",
    "static/communities.js",
    "static/community-edit-utils.js",
    "static/community-edit-danger.js",
    "static/community-edit-member-actions.js",
    "static/community-edit-members.js",
    "static/community-edit-category-actions.js",
    "static/community-edit-categories.js",
    "static/community-edit-form-utils.js",
    "static/community-edit-form.js",
    "static/community-edit-slack.js",
    "static/community-edit-discord.js",
    "static/community-edit-x.js",
    "static/community-edit-notifications.js",
    "static/community-edit-core.js",
    "static/members-only.js",
    "static/slack-oauth-handler.js",
    "static/api-tokens.js"
  ],
  "scss": [
    "static/style.scss"
  ],
  "modules": {
    "../admin/plugins/caiz-oauth.js": "static/lib/admin/plugins/caiz.js",
    "../admin/plugins/caiz-i18n.js": "static/lib/admin/plugins/caiz-i18n.js"
  },
  "acpScripts": [
    "static/lib/admin/plugins/caiz.js",
    "static/lib/admin/plugins/caiz-i18n.js"
  ],
  "hooks": [
    {
      "hook": "static:app.load",
      "method": "init"
    },
    {
      "hook": "filter:categories.build",
      "method": "customizeCategoriesLink"
    },
    {
      "hook": "filter:topic.build",
      "method": "customizeTopicRender"
    },
    {
      "hook": "filter:category.get",
      "method": "customizeCategoryLink"
    },
    {
      "hook": "filter:category.build",
      "method": "filterCategoryBuild"
    },
    {
      "hook": "filter:middleware.renderHeader",
      "method": "customizeSidebarCommunities"
    },
    {
      "hook": "filter:middleware.renderHeader",
      "method": "loadCommunityEditModal"
    },
    {
      "hook": "filter:topic.create",
      "method": "filterTopicCreate"
    },
    {
      "hook": "filter:post.create",
      "method": "filterPostCreate"
    },
    {
      "hook": "filter:recent.build",
      "method": "filterTopicsBuild"
    },
    {
      "hook": "filter:popular.build", 
      "method": "filterTopicsBuild"
    },
    {
      "hook": "filter:unread.build",
      "method": "filterTopicsBuild"
    },
    {
      "hook": "filter:admin.header.build",
      "method": "addAdminNavigation"
    },
    {
      "hook": "action:topic.save",
      "method": "actionTopicSave"
    },
    {
      "hook": "action:post.save",
      "method": "actionPostSave"
    }
  ]
}
