{
  "id": "nodebb-plugin-caiz",
  "name": "NodeBB Plugin for Caiz",
  "description": "NodeBB Plugin for Caiz",
  "version": "1.0.0",
  "library": "./library.js",
  "languages": "languages",
  "scripts": [
    "static/modal.js",
    "static/follow.js",
    "static/communities.js",
    "static/community-edit.js",
    "static/members-only.js"
  ],
  "scss": [
    "static/style.scss"
  ],
  "hooks": [
    {
      "hook": "static:app.load",
      "method": "init"
    },
    {
      "hook": "filter:categories.build",
      "method": "customizeCategoriesLink"
    },
    {
      "hook": "filter:topic.build",
      "method": "customizeTopicRender"
    },
    {
      "hook": "filter:category.get",
      "method": "customizeCategoryLink"
    },
    {
      "hook": "filter:middleware.renderHeader",
      "method": "customizeSidebarCommunities"
    },
    {
      "hook": "filter:middleware.renderHeader",
      "method": "loadCommunityEditModal"
    },
    {
      "hook": "filter:topic.create",
      "method": "filterTopicCreate"
    },
    {
      "hook": "filter:post.create",
      "method": "filterPostCreate"
    }
  ]
}